<h3>新增运行记录</h3>

<%= simple_form_for [@equipment_category, @transformer, @move_record] do |f| %>
  <%= f.input :year %>
  <%= f.input :operation_hours%>
  <%= f.input :overload_time%>
  <%= f.input :overload_cumulative_time%>
  <%= f.input :utilize_percentage%>
  <%= f.input :short_circuit_time%>
  <%= f.input :run_record%>

  <%= f.submit "提交", data: {:disable_with => "提交中..."} %>
<% end %>

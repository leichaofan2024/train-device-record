<%= simple_form_for [@equipment_category,:equipments,@discharge_coil] do |f| %>
  
  <%= f.select :factory_id, Factory.all.map{|u| [u.name,u.id]} %>
  <%= f.input :running_number %>
  <%= f.input :standard  %>
  <%= f.input :voltage_persent%>
  <%= f.input :rated_voltage  %>
  <%= f.input :half_capacity %>
  <%= f.input :one_capacity  %>
  <%= f.input :three_capacity %>
  <%= f.input :max_capacity  %>
  <%= f.input :insulation_oil %>
  <%= f.input :total_weight   %>
  <%= f.input :manufacture_date  %>
  <%= f.input :manufacture_number %>
  <%= f.input :commissioning_date  %>
  <%= f.input :remark %>
  <%= f.submit "提交", data: {disable_with: "提交中..."} %>
<% end %>

<%= simple_form_for [@equipment_category,:equipments,@current_transformer] do |f| %>
  <%= f.select :station_id, [[current_station.name,current_station.id]] %>
  <%= f.select :factory_id, Factory.all.map{|u| [u.name,u.id]} %>
  <%= f.input :running_number %>
  <%= f.input :standard %>
  <%= f.input :rated_voltage  %>
  <%= f.input :rated_current  %>
  <%= f.input :insulation_oil   %>
  <%= f.input :total_weight   %>
  <%= f.input :current_persent %>
  <%= f.input :first_class_coil  %>
  <%= f.input :first_coil_load %>
  <%= f.input :second_class_coil %>
  <%= f.input :second_coil_load  %>
  <%= f.submit "提交", data: {disable_with: "提交中..."} %>
<% end %>

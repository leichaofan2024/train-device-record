<%= simple_form_for [@equipment_category,:equipments,@battery] do |f| %>
  
  <%= f.select :factory_id, Factory.all.map{|u| [u.name,u.id]} %>
  <%= f.input :standard, label:"规格编号"%>
  <%= f.input :number, label: "组数"%>
  <%= f.input :rated_voltage, label:"整组额定电压"%>
  <%= f.input :single_voltage, label:"单只额定电压"%>
  <%= f.input :rated_capacity,label: "额定容量"%>
  <%= f.input :category, label:"类型"%>
  <%= f.input :manufacture_date,label: "出厂日期"%>
  <%= f.input :commissioning_date,label: "投运日期"%>
  <%= f.input :remark,label: "备注"%>
  <%= f.submit "提交", data: {disable_with: "提交中..."}%>
<% end %>

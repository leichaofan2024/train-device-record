<%= simple_form_for [@equipment_category,:equipments,@surge_arrester] do |f| %>
  
  <%= f.select :factory_id, Factory.all.map{|u| [u.name,u.id]} %>
  <%= f.input :standard, label:"规格编号"%>
  <%= f.input :running_number, label:"运行编号"%>
  <%= f.input :rated_voltage, label:"额定电压"%>
  <%= f.input :casing,label: "材质"%>
  <%= f.input :manufacture_date,label: "出厂日期"%>
  <%= f.input :commissioning_date,label: "投运日期"%>
  <%= f.input :remark,label: "备注"%>
  <%= f.submit "提交", data: {disable_with: "提交中..."}%>
<% end %>

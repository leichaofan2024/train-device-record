<%= simple_form_for [@equipment_category,:equipments,@circuit_breaker] do |f| %>
  
  <%= f.select :factory_id, Factory.all.map{|u| [u.name,u.id]} %>
  <%= f.input :running_number %>
  <%= f.input :standard %>
  <%= f.input :rated_voltage %>
  <%= f.input :orgnization_model %>
  <%= f.input :rated_current %>
  <%= f.input :rated_short %>
  <%= f.input :orgnization_sort %>
  <%= f.input :current_transformer_type %>
  <%= f.input :current_transformer_percent %>
  <%= f.input :gas_pressure %>
  <%= f.input :total_weight %>
  <%= f.input :status %>
  <%= f.input :manufacture_number %>
  <%= f.input :manufacture_date %>
  <%= f.input :commissioning_date %>
  <%= f.input :remark %>
  <%= f.submit "提交", data: {disable_with: "提交中..."} %>
<% end %>
